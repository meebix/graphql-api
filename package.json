{
   "name": "node-api",
   "version": "1.0.0",
   "description": "Fully Featured Node.js API with SQL DB",
   "author": "Mike Donahue <mtdonahue1@gmail.com>",
   "scripts": {
      "start:dev": "node dist/app.js | bunyan",
      "start": "NODE_ENV=production node dist/app.js | bunyan",
      "nodemon": "nodemon src/app.js --exec babel-node | bunyan",
      "build": "babel src -d dist --copy-files",
      "test": "NODE_ENV=test nyc mocha 'src/**/*.test.js'",
      "test:bb": "NODE_ENV=test mocha 'src/**/*.test.bb.js' --timeout 30000 --exit",
      "test:watch": "NODE_ENV=test nodemon --exec \"mocha 'src/**/*.test.js'\"",
      "lint": "eslint 'config/*.js' 'migrations/*.js' 'seeders/*.js' 'src/**/*.js'",
      "migrate:dev": "NODE_ENV=development knex migrate:latest",
      "migrate:undo:dev": "NODE_ENV=development knex migrate:rollback",
      "seed:dev": "NODE_ENV=development knex seed:run",
      "migrate": "NODE_ENV=production knex migrate:latest",
      "migrate:undo": "NODE_ENV=production knex migrate:rollback",
      "seed": "NODE_ENV=production knex seed:run",
      "mysql:restart": "mysql.server restart"
   },
   "nyc": {
      "check-coverage": true,
      "lines": 75,
      "statements": 75,
      "functions": 75,
      "branches": 75,
      "include": [
         "src/api/**",
         "src/middleware/**",
         "!src/models/**"
      ],
      "reporter": [
         "lcov",
         "text-summary"
      ]
   },
   "main": "src/app.js",
   "repository": {
      "type": "git",
      "url": "https://github.com/sidewalksalsa/node-api.git"
   },
   "dependencies": {
      "accesscontrol": "^1.5.3",
      "apollo-server-express": "^1.4.0",
      "app-module-path": "^2.2.0",
      "argon2": "^0.19.3",
      "bcrypt": "^1.0.2",
      "body-parser": "^1.18.1",
      "bunyan": "^1.8.10",
      "bunyan-prettystream": "^0.1.3",
      "class-validator": "^0.9.1",
      "config": "^1.30.0",
      "cookie-parser": "^1.4.3",
      "cors": "^2.8.3",
      "csurf": "^1.9.0",
      "date-fns": "^1.29.0",
      "express": "^4.15.4",
      "express-healthcheck": "^0.1.0",
      "graphql": "^0.13.2",
      "graphql-resolvers": "^0.2.2",
      "graphql-tools": "^3.0.5",
      "helmet": "^3.12.1",
      "jsonwebtoken": "^8.3.0",
      "knex": "^0.15.1",
      "lodash.difference": "^4.5.0",
      "md5": "^2.2.1",
      "morgan": "^1.8.2",
      "morgan-json": "^1.1.0",
      "mysql2": "^1.5.3",
      "object-path": "^0.11.4",
      "pretty-hrtime": "^1.0.3",
      "reflect-metadata": "^0.1.10",
      "response-time": "^2.3.2",
      "sparkpost": "^2.1.2",
      "stripe": "^5.5.0",
      "twilio": "^3.17.6",
      "typeorm": "0.2.7",
      "uuid": "^3.3.2",
      "validate.js": "^0.12.0",
      "verror": "^1.10.0"
   },
   "devDependencies": {
      "babel-cli": "^6.26.0",
      "babel-core": "^6.26.3",
      "babel-eslint": "^8.2.6",
      "babel-plugin-add-module-exports": "^0.3.2",
      "babel-plugin-transform-class-properties": "^6.24.1",
      "babel-plugin-transform-decorators-legacy": "^1.3.5",
      "babel-plugin-transform-object-rest-spread": "^6.26.0",
      "babel-preset-env": "^1.7.0",
      "chai": "^4.1.2",
      "chance": "^1.0.16",
      "eslint": "^4.6.1",
      "eslint-config-airbnb-base": "^11.3.2",
      "eslint-plugin-import": "^2.7.0",
      "mocha": "^5.2.0",
      "nodemon": "^1.18.2",
      "nyc": "^11.2.1",
      "proxyquire": "^2.0.1",
      "sinon": "^5.0.7",
      "supertest": "^3.1.0"
   },
   "private": true,
   "license": "MIT"
}
