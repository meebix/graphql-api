{
  "name": "teach-learn-api",
  "version": "1.0.0",
  "description": "Teach and Learn Anything",
  "author": "Make It Count <mike@makeitcount.cc>",
  "scripts": {
    "start:dev": "node src/app.js | bunyan",
    "start:prod": "NODE_ENV=production node src/app.js | bunyan",
    "nodemon": "nodemon src/app.js | bunyan",
    "test": "NODE_ENV=test nyc mocha 'src/**/*.test.js'",
    "test:watch": "NODE_ENV=test nodemon --exec \"mocha 'src/**/*.test.js'\"",
    "test:bb": "NODE_ENV=test mocha 'src/**/*.test.bb.js' --timeout 30000",
    "lint": "eslint 'config/*.js' 'migrations/*.js' 'seeders/*.js' 'src/**/*.js'",
    "migrate": "sequelize db:migrate --config config/databases.js",
    "migrate:undo": "sequelize db:migrate:undo --config config/databases.js",
    "migrate:undo:all": "sequelize db:migrate:undo:all --config config/databases.js",
    "seed": "sequelize db:seed:all --config config/databases.js",
    "seed:undo": "sequelize db:seed:undo --config config/databases.js",
    "seed:undo:all": "sequelize db:seed:undo:all --config config/databases.js"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "watermarks": {
      "lines": [
        75,
        95
      ],
      "functions": [
        75,
        95
      ],
      "branches": [
        75,
        95
      ],
      "statements": [
        75,
        95
      ]
    },
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sidewalksalsa/teach-learn-api.git"
  },
  "dependencies": {
    "accesscontrol": "^1.5.3",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "bunyan-prettystream": "^0.1.3",
    "config": "^1.26.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "csurf": "^1.9.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "jsonwebtoken": "^8.0.0",
    "lodash.difference": "^4.5.0",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "morgan-json": "^1.1.0",
    "mysql2": "^1.4.2",
    "object-path": "^0.11.4",
    "pretty-hrtime": "^1.0.3",
    "response-time": "^2.3.2",
    "sequelize": "^4.8.2",
    "sequelize-cli": "^3.0.0-0",
    "shortid": "^2.2.8",
    "sparkpost": "^2.1.2",
    "swagger-node-express": "^2.1.3",
    "verror": "^1.10.0",
    "xss": "^0.3.4"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chance": "^1.0.11",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "lodash.times": "^4.3.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.12.0",
    "nyc": "^11.2.1",
    "proxyquire": "^1.8.0",
    "sinon": "^4.0.0",
    "supertest": "^3.0.0"
  },
  "private": true,
  "license": "MIT"
}
