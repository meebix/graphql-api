{
  "name": "node-api",
  "version": "1.0.0",
  "description": "A Node.js REST API built using Express.js, TypeORM, and MySQL",
  "author": "Mike Donahue <mtdonahue1@gmail.com>",
  "scripts": {
    "build": "babel src --out-dir dist --source-maps --copy-files",
    "start:dev": "node dist/app.js | bunyan",
    "start": "NODE_ENV=production node dist/app.js | bunyan",
    "watch": "nodemon --exec 'yarn build && yarn start:dev | bunyan'",
    "test": "NODE_ENV=test nyc mocha 'src/**/*.test.js'",
    "test:bb": "NODE_ENV=test mocha 'src/**/*.test.bb.js' --timeout 30000 --exit",
    "test:watch": "NODE_ENV=test nodemon --exec \"mocha 'src/**/*.test.js'\"",
    "lint-js": "eslint 'config/*.js' 'seeders/*.js' 'src/**/*.js'",
    "db:migrate": "typeorm migration:run",
    "db:migrate:undo": "typeorm migration:revert",
    "db:sync:log": "typeorm schema:log",
    "db:sync": "typeorm schema:sync",
    "db:drop": "typeorm schema:drop",
    "seed": "ts-node -r tsconfig-paths/register",
    "mysql:restart": "mysql.server restart"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "include": [
      "src/api/**",
      "src/middleware/**",
      "!src/models/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sidewalksalsa/node-api.git"
  },
  "dependencies": {
    "accesscontrol": "1.5.3",
    "app-module-path": "2.2.0",
    "argon2": "0.19.3",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "bunyan-prettystream": "0.1.3",
    "chance": "^1.0.16",
    "class-validator": "0.9.1",
    "config": "1.30.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "csurf": "1.9.0",
    "date-fns": "1.29.0",
    "express": "4.16.3",
    "express-healthcheck": "0.1.0",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.difference": "4.5.0",
    "md5": "2.2.1",
    "morgan": "1.9.0",
    "morgan-json": "1.1.0",
    "mysql2": "1.6.1",
    "object-path": "0.11.4",
    "pretty-hrtime": "1.0.3",
    "reflect-metadata": "0.1.12",
    "response-time": "2.3.2",
    "rimraf": "2.6.2",
    "source-map-support": "^0.5.8",
    "sparkpost": "2.1.2",
    "stripe": "6.7.0",
    "twilio": "3.19.1",
    "typeorm": "^0.3.0-alpha.10",
    "uuid": "3.3.2",
    "verror": "1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "chai": "4.1.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.3",
    "nyc": "11.2.1",
    "proxyquire": "2.0.1",
    "sinon": "5.0.7",
    "supertest": "3.1.0"
  },
  "private": true,
  "license": "MIT"
}
