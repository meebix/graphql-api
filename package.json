{
  "name": "teach-learn-api",
  "version": "1.0.0",
  "description": "Teach and Learn Anything",
  "author": "Make It Count <mike@makeitcount.cc>",
  "scripts": {
    "start:dev": "node src/app.js | bunyan",
    "start:prod": "NODE_ENV=production node src/app.js | bunyan",
    "nodemon": "nodemon src/app.js | bunyan",
    "test": "node_modules/mocha/bin/mocha \"src/**/*.js\"",
    "lint": "eslint 'config/*.js' 'src/**/*.js'",
    "migrate": "sequelize db:migrate --config config/databases.js",
    "migrate:undo": "sequelize db:migrate:undo --config config/databases.js",
    "migrate:undo:all": "sequelize db:migrate:undo:all --config config/databases.js"
  },
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sidewalksalsa/teach-learn-api.git"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "config": "^1.26.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "csurf": "^1.9.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "jsonwebtoken": "^7.4.1",
    "lodash.difference": "^4.5.0",
    "md5": "^2.2.1",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "morgan-json": "^1.1.0",
    "mysql": "^2.13.0",
    "mysql2": "^1.4.2",
    "object-path": "^0.11.4",
    "sequelize": "^4.7.5",
    "sequelize-cli": "^2.7.0",
    "shortid": "^2.2.8",
    "sparkpost": "^2.1.2",
    "swagger-node-express": "^2.1.3",
    "verror": "^1.10.0",
    "xss": "^0.3.3"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "private": true,
  "license": "MIT"
}
